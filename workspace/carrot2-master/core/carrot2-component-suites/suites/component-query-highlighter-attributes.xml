<attribute-sets default="attributes">
  <attribute-set id="attributes">
    <value-set>
      <label>attributes</label>

      <attribute key="QueryWordHighlighter.enabled">
        <value value="true" />
      </attribute>
      
      <attribute key="QueryWordHighlighter.querySanitizePattern">
        <value value="[&quot;'()+\-!]" />
      </attribute>

      <attribute key="QueryWordHighlighter.dontHighlightPattern">
        <value value="(?i:and)|(?i:or)|(?i:not)" />
      </attribute>
    </value-set>
  </attribute-set>
</attribute-sets>
