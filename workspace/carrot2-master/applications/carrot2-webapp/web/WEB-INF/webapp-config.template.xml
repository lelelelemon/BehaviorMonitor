<?xml version="1.0" encoding="UTF-8"?>
<config skins-folder="skins@VERSION@" component-suite="suite-webapp.xml">
  <skins>
    <skin id="fancy-large" sprited="true" />
    <skin id="fancy-compact" sprited="true" default="true" />
    <skin id="simple" sprited="true" />
  </skins>
  <sizes>
    <size size="50" />
    <size size="100" default="true" />
    <size size="150" />
    <size size="200" />
  </sizes>
  <views>
    <view id="folders" default="true">
      <label>Folders</label>
    </view>
    <view id="circles">
      <label>Circles</label>
    </view>
    <view id="foamtree">
      <label>FoamTree</label>
    </view>
  </views>
  <caches>
    <cache component="org.carrot2.core.IDocumentSource"/>
  </caches>
</config>
